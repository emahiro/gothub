<html><head></head><body><h1>Search</h1><p>input search keywords which you want to search</p><div id="search-form">	<form action="/github/search/repositories" method="POST">      <label for="forSearchQuery">QUERY</label>      <input type="text" name="q">      <input class="search-btn" type="button" value="検索">	</form></div><div id="search-result"></div></body><script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script></html><script>    $(".search-btn").on("click",function () {      var url = $("#search-form form").attr("action");      var p = $("#search-form form").serialize()      $.ajax({        url: url,        type: "post",        data: p,      }).done(function (data) {        res = JSON.parse(data);        items = res.items;        Object.keys(items).forEach(function (idx) {            item = items[idx];            var resultHtml = $("#search-result");            resultHtml.append("<p>ID:"+ item.id + "</p>");	        resultHtml.append("<p>FullName: <b>"+ item.full_name + "</b></p>");	        resultHtml.append("<p>Owner: "+ item.owner.login + "</p>");	        resultHtml.append("<a href=" + item.html_url + " target='_blank'>URL: " + item.html_url + "</a>");        });      }).fail(function (err) {        console.log(err)      });    });</script>