<html><head></head><body><h1>Search</h1><p>input search keywords which you want to search</p><div id="search-form">	<form action="/github/search/repositories" method="POST">      <label for="forSearchQuery">QUERY</label>      <input type="text" name="q">      <input class="search-btn" type="button" value="検索">	</form></div><div id="search-result"></div>{{range $.Items}}<p>ID: {{.ID}}</p><p>Name: {{.Name}}</p><p>FullName: {{.FullName}}</p><p>URL: <a href="{{.HtmlUrl}}" target="_blank">{{.HtmlUrl}}</a></p>{{end}}</body><script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script></html><script>    $(".search-btn").on("click",function () {      var url = $("#search-form form").attr("action");      var p = $("#search-form form").serialize()      console.log(p)      $.ajax({        url: url,        type: "post",        data: p,      }).done(function (data) {        console.log(data)      }).fail(function (err) {        console.log(err)      });    });</script>